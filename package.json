{
  "name": "auto-tests",
  "version": "1.0.3",
  "private": true,
  "description": "api and browser tests",
  "scripts": {
    "test:api": "npx mocha api/**/*.ts --reporter-options reportFilename=api,reportTitle='api tests',reportPageTitle='api tests'",
    "test:chrome": "npx mocha browser/specs/**/*.ts --reporter-options reportFilename=chrome,reportTitle='chrome tests',reportPageTitle='chrome tests'",
    "test:all": "npx mocha 'api/**/*.spec.ts' 'browser/**/*.spec.ts' --reporter-options reportFilename=all,reportTitle='all tests',reportPageTitle='all tests'"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/selenium-webdriver": "^4.0.11",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chromedriver": "^89.0.0",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "prettier": "^2.2.1",
    "selenium-webdriver": "^4.0.0-beta.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
