{
  "name": "auto-tests",
  "version": "1.6.5",
  "private": true,
  "description": "rest api and selenium (chrome) tests",
  "scripts": {
    "run-api-tests": "npx mocha e2e/api/**/*.ts",
    "run-browser-tests": "npx mocha e2e/ui/tests/**/*.ts",
    "run-all-tests": "npx mocha e2e/api/**/*.ts e2e/ui/**/*.test.ts",
    "format-code": "npx prettier --ignore-path .gitignore **/*.{ts,json,md} --write",
    "update-all-dependencies": "npx ncu -u && npm i"
  },
  "dependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.1",
    "@types/selenium-webdriver": "^4.0.15",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chromedriver": "^91.0.1",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.2",
    "mochawesome": "^6.2.2",
    "npm-check-updates": "^11.8.2",
    "prettier": "^2.3.2",
    "selenium-webdriver": "^4.0.0-beta.4",
    "ts-node": "^10.1.0"
  }
}
